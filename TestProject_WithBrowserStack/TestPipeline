{
	agent any
	
	tools{
	    git credentialsId: 'GitHub', url: 'https://github.com/Rajesh91Bhatia/TestBrowserStack.git'
	}

	
	stages
	{
		stage('Test Stage 1')
		{
			steps
			{
				echo "First step"
			}
	
		}
    }
    post
    {
    	always
    	{
    		echo "This will always run";
    	}
    	success
    	{
    		echo "This will run on success";
    	}
	}
}



    

